- for (var i=0; i<results.length; i++)
  .panel.panel-body
    .col-sm-4.pull-left.vcenter
      img(src='#{results[i].smallIcon}', class="img-responsive")
    .col-sm-8.text-center.vcenter
      strong
        a(href='#{results[i].shortName}', style="color: black") #{results[i].name}
      div.text-left #{results[i].shortDescription}